{
	"oauth": {
		"google": {
			"issure": "https://accounts.google.com",
			"redirect_uri": "https://vitevue3.com/oauth2/callback",
			"scopes": ["openid", "email", "profile", "https://www.googleapis.com/auth/calendar.readonly"],
			"access_type": "offline"
		}
	},
	"redis": {
		"session": {
			"port": 6379,
			"host": "localhost",
			"db": 1
		}
	},
	"session": {
		"name": "express.sid",
		"resave": false,
		"saveUninitialized": true,
		"cookie": {
			"httpOnly": true,
			"secure": false,
			"maxAge": 604800000
		}
	}
}
